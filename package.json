    "nyc": "^15.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.2",
    "check-code-coverage": "^1.10.0",
    "cypress": "^6.5.0",
    "jsdoc": "^3.6.6",
    "start-server-and-test": "^1.12.0"
  }
